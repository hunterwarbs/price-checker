# OpenRouter API Key - Get from https://openrouter.ai/
OPENROUTER_API_KEY=sk-or-your-api-key-here

# Fireworks AI Vision OCR (Qwen2.5-VL)
FIREWORKS_API_KEY=your-fireworks-api-key
FIREWORKS_BASE_URL=https://api.fireworks.ai/inference/v1
FIREWORKS_VL_MODEL=accounts/fireworks/models/qwen2p5-vl-32b-instruct

# Cloudflare Tunnel Token for external access via docker compose production profile
# Obtain from Cloudflare Zero Trust > Access > Tunnels
CLOUDFLARE_TUNNEL_TOKEN=your-tunnel-token-here

# Oxylabs Proxy Credentials (for screenshots/browser automation) - Format: username:password
OXYLABS_PROXY=your-proxy-username:your-proxy-password

# Oxylabs Web API Credentials (for Google search) - Format: username:password
OXYLABS_WEB_API=your-web-api-username:your-web-api-password

# Worker Pool Size - Number of concurrent workers for processing items (default: 20, max: 200)
WORKER_POOL_SIZE=20

# OpenRouter Rate Limit - Max concurrent requests to OpenRouter API (default: 200)
# WARNING: Set this based on your OpenRouter plan limits to avoid rate limiting
OPENROUTER_MAX_REQUESTS_PER_MINUTE=200

# Smart Rate Limiter Settings (Advanced users only - defaults are research-based)
# These settings override the smart rate limiter defaults if needed
# SMART_OPENROUTER_INITIAL_RPS=3.0
# SMART_OPENROUTER_MAX_RPS=10.0
# SMART_EXA settings removed - Exa API no longer used
# SMART_OXYLABS_PROXY_INITIAL_RPS=10.0    # For screenshots/browser automation
# SMART_OXYLABS_PROXY_MAX_RPS=25.0
# SMART_OXYLABS_WEB_API_INITIAL_RPS=45.0  # For Google search API calls
# SMART_OXYLABS_WEB_API_MAX_RPS=50.0 